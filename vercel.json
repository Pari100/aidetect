{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "env": {
    "NODE_ENV": "production"
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/dist/index.cjs"
    },
    {
      "source": "/(.*)",
      "destination": "/dist/index.cjs"
    }
  ]
}
